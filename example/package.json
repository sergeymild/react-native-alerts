{
  "name": "react-native-alerts-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build:android": "cd android && ./gradlew assembleDebug --no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a",
    "build:ios": "cd ios && xcodebuild -workspace AlertsExample.xcworkspace -scheme AlertsExample -configuration Debug -sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.76.5"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^5.0.2",
    "metro-react-native-babel-preset": "0.77.0",
    "patch-package": "^8.0.0",
    "@types/react": "18.3.12",
    "@types/react-native": "0.73.0",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "16.0.0",
    "@react-native-community/cli-platform-android": "16.0.0",
    "@react-native-community/cli-platform-ios": "^16.0.0",
    "@react-native/babel-preset": "0.76.3",
    "@react-native/metro-config": "0.76.3",
    "@react-native/typescript-config": "0.76.3",
    "react-native-builder-bob": "^0.33.3"
  },
  "engines": {
    "node": ">=18"
  }
}
